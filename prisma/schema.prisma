generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @unique @default(dbgenerated("uuid_generate_v4()"))
  name      String   @db.VarChar(50)
  email     String   @unique
  password  String
  createdAt DateTime @default(now()) @db.Timestamptz()
}
