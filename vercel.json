{
    "version": 3,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 0,12 * * *"
        }
    ]
}
